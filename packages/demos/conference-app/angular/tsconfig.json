{
  "compileOnSave": false,
  "compilerOptions": {
    "allowUnreachableCode": true,
    "alwaysStrict": false,
    "declaration": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": ".tmp",
    "pretty": true,
    "removeComments": false,
    "target": "es2015"
  }
}
